{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://ntq.onrender.com/api/$1"
        },
        {
            "source": "/login",
            "destination": "/index.html"
        },
        {
            "source": "/signup",
            "destination": "/index.html"
        },
        {
            "source": "/",
            "destination": "/index.html"
        }
    ]
}